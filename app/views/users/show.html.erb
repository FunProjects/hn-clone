<%= paginate @links %>
<table class ="table table-striped">
	<thead>
		<tr>
			<td>
				Title
			</td>
			<td>
				Link
			</td>
		</tr>
	</thead>
<% @links.each do |article|%>
	<tr>
		<td><%= link_to article.title, article  %></td>
		<td><%= link_to article.url, article.url %></td>
		<td><%= link_to "Edit", edit_link_path(article.id) %></td>
	</tr>
<% end %>
</table>
<%= paginate @links %>
